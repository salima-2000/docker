pipeline {
environment {
registry = "salima123/my-app.jar"
registryCredential = 'salima123'
dockerImage = 'image'
}
agent any
stages {
 stage('Build') {
 steps {
 sh â€˜mvn package'
 }
 }
stage('Building image') {
steps{
 script {
 dockerImage = docker.build registry + ":$BUILD_NUMBER"
 }
 }
 }
stage('Deploy Image') {
steps{
 script {
 docker.withRegistry( '', registryCredential ) {
 dockerImage.push()
 }
 }
 }
}
}
}
